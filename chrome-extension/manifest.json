{
    "manifest_version": 3,
    "name": "Read it Later",
    "version": "0.4",
    "description": "Send URL and tags to Read it Later.",
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}